{
  "name": "cla",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "export NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "1.3.4",
    "core-js": "3.6.5",
    "element-ui": "2.15.12",
    "file-saver": "2.0.2",
    "js-base64": "3.7.5",
    "js-cookie": "3.0.1",
    "less-loader": "6.1.1",
    "regenerator-runtime": "0.13.11",
    "register-service-worker": "1.7.2",
    "vue": "2.7.10",
    "vue-i18n": "8.27.2",
    "vue-markdown": "2.2.4",
    "vue-pdf": "^4.2.0",
    "vue-router": "3.6.5",
    "vuex": "3.6.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-plugin-router": "~4.4.0",
    "@vue/cli-plugin-unit-mocha": "~4.4.0",
    "@vue/cli-plugin-vuex": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.2",
    "file-loader": "^6.0.0",
    "svg-sprite-loader": "^5.0.0",
    "vue-template-compiler": "2.7.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "es6": true,
      "node": true,
      "browser": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parser": "vue-eslint-parser",
    "parserOptions": {
      "ecmaVersion": 2018,
      "parser": "babel-eslint",
      "sourceType": "module"
    },
    "rules": {
      "no-undef": "off",
      "no-unused-vars": "off"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "mocha": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "nanachi": {
    "alias": {
      "@assets": "src/assets"
    }
  }
}
